# ed26a987d18b4fd3a6a6d9ee72cc91fc
msgid ":ref:`agg-helper-incompatibility`"
msgstr ""

#: ../source/release-notes/2.6.txt:272
# df6bdc0cfa3543c99e0cf2cfc3530adb
msgid "``$out`` Stage to Write Data to a Collection"
msgstr ""

#: ../source/release-notes/2.6.txt:276
# 4e320838ee4f4a369f4c25b0a4300d5b
msgid "The aggregation pipeline adds a new stage named :pipeline:`$out` that writes the result of the pipeline operation to an output collection and returns an empty result set. In version 2.5.3 of the :program:`mongo` shell, because the :method:`db.collection.aggregate()` method returns a cursor, specifying an :pipeline:`$out` stage returns an empty cursor."
msgstr ""

#: ../source/release-notes/2.6.txt:282
# b1948b8a7ea742eb9849a6bd71dbe41f
msgid "See the :doc:`$out documentation </reference/operator/aggregation/out>` for more information."
msgstr ""

#: ../source/release-notes/2.6.txt:288
# 9fa47be1b524437db7d6cebb281a5f9c
msgid "Aggregation Operations Now Return Cursors"
msgstr ""

#: ../source/release-notes/2.6.txt:294
# 77465fe920b94ea89bf88c092a8767da
msgid "The :method:`db.collection.aggregate()` helper in the :program:`mongo` shell now returns a cursor. By returning a cursor, aggregation pipelines can return result sets of any size. In previous versions, the result of an aggregation operation could be no larger than 16 megabytes. See :method:`db.collection.aggregate()`."
msgstr ""

#: ../source/release-notes/2.6.txt:300
# cd3951c36e17492cacbb50f97a976e65
msgid "The :dbcommand:`aggregate` command can return a document that contains results with which to instantiate a cursor object.  See :dbcommand:`aggregate`."
msgstr ""

#: ../source/release-notes/2.6.txt:304
# bad6aba2b4a44eb9bbc63b95cb8bb7ca
msgid "``explain`` Option for the Aggregation Pipeline"
msgstr ""

#: ../source/release-notes/2.6.txt:306
# 2f06bf10c7f844a3bc135da5c9cfdc7a
msgid "The new ``explain`` option for aggregation provides information about how :program:`mongod` processes the pipeline. See :method:`db.collection.aggregate()` and :dbcommand:`aggregate` for details."
msgstr ""

#: ../source/release-notes/2.6.txt:312
# 67596fe13d464d2ebf342562ce5f14a3
msgid "Improved Sorting"
msgstr ""

#: ../source/release-notes/2.6.txt:316
# 41e4c012d6854e729a0d1868a209779a
msgid "The :pipeline:`$sort` and :pipeline:`$group` stages now use a more efficient sorting system that provides improved performance."
msgstr ""

#: ../source/release-notes/2.6.txt:319
# b58263b5caa040a19188bddfd702409a
msgid "The new ``allowDiskUsage`` option enables stages to write data to temporary files. See :method:`db.collection.aggregate()` and :dbcommand:`aggregate`."
msgstr ""

#: ../source/release-notes/2.6.txt:324
# 9f2ed29ad987410a91dea750fcb3f56a
msgid "``$redact`` Stage to Provide Filtering for Field-Level Access Control"
msgstr ""

#: ../source/release-notes/2.6.txt:328
# 9421d82bc24a4dca9f6c8a2280cd9ea6
msgid "The :pipeline:`$redact` stage restricts the content of a returned document on a per-field level. See :pipeline:`$redact`."
msgstr ""

#: ../source/release-notes/2.6.txt:332
# c8735f2e633a48249747f0e9e237ca4e
msgid "Set Expression Operations in Aggregation Pipeline Stages"
msgstr ""

#: ../source/release-notes/2.6.txt:334
# bae073fe2ac34155bcfb41b40d5401b6
msgid "In 2.5.2, the :pipeline:`$project` and :pipeline:`$redact` aggregation pipeline stages now support a collection of :doc:`set expression operators </reference/operator/aggregation-set>`."
msgstr ""

#: ../source/release-notes/2.6.txt:339
# 66b1140bd7e849d9bda5596021fde0b3
msgid "``$map`` and ``$let`` Expressions in Aggregation Pipeline Stages"
msgstr ""

#: ../source/release-notes/2.6.txt:346
# 9488abb6da99437ba9373c200146e13c
msgid "The :expression:`$let` and :expression:`$map` make it possible to declare and manipulate variables within an aggregation pipeline stages, specifically the :pipeline:`$project`, :pipeline:`$group`, and :pipeline:`$redact` stages. See: :doc:`/reference/operator/aggregation/let` and :doc:`/reference/operator/aggregation/map` for more information."
msgstr ""

#: ../source/release-notes/2.6.txt:353
# b96b865579ac41edb6b4d56e264e519b
msgid "``$literal`` Expression for Aggregation Pipeline Stages"
msgstr ""

#: ../source/release-notes/2.6.txt:355
# cdf1480ccc974740b37efa67399a98fe
msgid "The new :expression:`$literal` operator allows users to explicitly specify documents in aggregation operations that the pipeline stage would otherwise interpret directly. See :doc:`/reference/operator/aggregation/literal` for more information."
msgstr ""

#: ../source/release-notes/2.6.txt:361
# d37f24b45a1449bd8daf559fc14d888f
msgid "``$cond`` Accepts Objects as Arguments"
msgstr ""

#: ../source/release-notes/2.6.txt:365
# 9275560780a74d16889efdf18ad2f610
msgid "The ternary :expression:`$cond` expression can now take either an object or an array. Previously :expression:`$cond` always took an array. See :doc:`/reference/operator/aggregation/cond` for more information."
msgstr ""

#: ../source/release-notes/2.6.txt:370
# 82e80f74f11640a8922638f717253ea0
msgid "New ``$size`` Operator for the Aggregation Pipeline"
msgstr ""

#: ../source/release-notes/2.6.txt:374
# d034fe4f2cce44bea7f2203ee07e0df8
msgid "The new :expression:`$size` operator for the :doc:`aggregation pipeline </core/aggregation-pipeline>` returns the size of a specified array. See :doc:`/reference/operator/aggregation/size` for more information."
msgstr ""

