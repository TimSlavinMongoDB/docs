# 1455d84ef69a4735ab39687efcf877e5
msgid "Changes"
msgstr ""

#: ../source/release-notes/2.6.txt:257
# 849e5d539b564115b652786b0781f8ce
msgid "Aggregation Pipeline Changes"
msgstr ""

#: ../source/release-notes/2.6.txt:262
# d29b2b9451764fb79a651fea3d16b8db
msgid "``db.collection.aggregation()`` Accepts Second Parameter"
msgstr ""

#: ../source/release-notes/2.6.txt:264
# ac29e37b6dfc4305916e721911231ad2
msgid "In the 2.5.3 version of the :program:`mongo` shell, the :method:`db.collection.aggregate()` helper can now accept as a second parameter a document of options. See :method:`db.collection.aggregate()`."
msgstr ""

