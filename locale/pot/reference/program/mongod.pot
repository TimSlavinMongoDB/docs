msgstr ""

#: ../source/reference/program/mongod.txt:145
# d7ef6ed1b175488da91550875b20a817
msgid "When specified, this option ensures that :program:`mongod` appends new entries to the end of the logfile rather than overwriting the content of the log when the process restarts."
msgstr ""

#: ../source/reference/program/mongod.txt:153
# 2ea8abc909824b739c63a2646c11042c
msgid "Sends all logging output to the host's :term:`syslog` system rather than to standard output or a log file as with :option:`--logpath`."
msgstr ""

#: ../source/reference/program/mongod.txt:156
# 0bc352fed17d40a4a69713572f1dfc81
msgid "You cannot use :option:`--syslog` with :option:`--logpath`."
msgstr ""

#: ../source/reference/program/mongod.txt:160
# 83a46a2627d04a54b109cab640b281de
