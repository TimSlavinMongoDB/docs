# c75abec7781b4594a0c7b063d64bae04
msgid "**Level**"
msgstr ""

#: ../source/reference/program/mongod.txt:388
# 68b38cf1ca77481593d74a4ed283cf5e
msgid "Off. No profiling."
msgstr ""

#: ../source/reference/program/mongod.txt:389
# 1ee332add7db4ae78b74bcf68634c33a
msgid "On. Only includes slow operations."
msgstr ""

#: ../source/reference/program/mongod.txt:390
# 14fc5e86f7724eea9ec04fcff3bf0720
msgid "On. Includes all operations."
msgstr ""

#: ../source/reference/program/mongod.txt:393
# 45490975bf76490292948a284f2563a2
msgid "Profiling is off by default. Database profiling can impact database performance. Enable this option only after careful consideration."
msgstr ""

#: ../source/reference/program/mongod.txt:398
# c7ee2793e7d7418580d63033dd19c4fa
msgid "Enables a maximum limit for the number data files each database can have. When running with :option:`--quota`, there are a maximum of 8 data files per database. Adjust the quota with the :option:`--quotaFiles` option."
